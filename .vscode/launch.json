            "name": "Client (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Presentation/Scheduler/Client/bin/Debug/netcoreapp3.1/Scheduler.Client.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Presentation/Scheduler/Client",
            "console": "internalConsole",
            "stopAtEntry": false
